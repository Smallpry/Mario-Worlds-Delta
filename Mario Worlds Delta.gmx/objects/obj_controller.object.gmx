draw_rectangle_colour(0, 0, global.gw*(filter+1), global.gh*(filter+1), c_black, c_black, c_black, c_black, 0);

//Draw the application surface
draw_surface_stretched(application_surface, 0, 0, global.gw*(filter+1), global.gh*(filter+1));

//Reset the shader
shader_reset();

//Enable alpha blending
draw_enable_alphablend(1);

//Fades the screen to and from black
if (fade &lt; 1) {

    //Increment fading
    fade += 1/30;

    //Fade from black
    if (mode = 0) {
    
        draw_set_alpha(1-fade);
        size -= 0.75;
