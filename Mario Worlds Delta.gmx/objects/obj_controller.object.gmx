shader_set_uniform_f(uni_shock_width, var_shock_width);

//If the screen is fading
if (fade &lt; 1) 
&amp;&amp; (size &gt; 0.75) {
    
    shader_set(shd_pixel);
    shader_set_uniform_f(pixel, global.gw, global.gh, size, size);
}

